{
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "parser": "babel-eslint",
    "rules": {
      "camelcase": 0,
      "max-len": [
        "warn",
        124
      ],
      "comma-dangle": ["error", "never"],
      "indent": [
        "warn",
        2
      ],
      "semi": [
        2,
        "always"
      ],
      "no-confusing-arrow": "warn",
      "no-lone-blocks": "error",
      "no-multi-spaces": "error",
      "no-dupe-else-if": "warn",
      "arrow-parens": "warn",
      "default-param-last": "error",
      "default-case": "error",
      "curly": "error",
      "complexity": ["error", 24],
  //    "semi": 1
    }
  }